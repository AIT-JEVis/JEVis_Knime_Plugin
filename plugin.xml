<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<?eclipse version="3.0"?><plugin>

   <!-- This is an example for providing an own category 
      <extension
         point="org.knime.workbench.repository.categories">
      <category
            description="Category containing my nodes"
            icon="icons/default.png"
            level-id="myNodes"
            name="My nodes"
            path="/"/>
   </extension>
   -->
   
   <extension point="org.knime.workbench.repository.nodes">
      <node category-path="/" factory-class="org.ait.knimejevisplugin.getdata.JevisGetDataNodeFactory" id="org.ait.knime.getdata.JevisGetDataNodeFactory"/>
   <node category-path="/" factory-class="org.ait.knimejevisplugin.selectdata.JevisSelectDataNodeFactory" id="org.ait.knime.selectdata.JevisSelectDataNodeFactory"/>
<node category-path="/" factory-class="org.ait.knimejevisplugin.writedata.JevisWriteDataNodeFactory" id="org.ait.knime.writedata.JevisWriteDataNodeFactory"/>
</extension>
   
</plugin>
